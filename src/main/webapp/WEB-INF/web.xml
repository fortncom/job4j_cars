<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns="http://java.sun.com/xml/ns/j2ee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
         http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
         version="2.4">
  <display-name>car trade</display-name>
  <welcome-file-list>
    <welcome-file>index.do</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>IndexServlet</servlet-name>
    <servlet-class>ru.job4j.cartrade.controller.IndexServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AuthServlet</servlet-name>
    <servlet-class>ru.job4j.cartrade.controller.AuthServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>ru.job4j.cartrade.controller.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegServlet</servlet-name>
    <servlet-class>ru.job4j.cartrade.controller.RegServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CarServlet</servlet-name>
    <servlet-class>ru.job4j.cartrade.controller.CarServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DownloadPhotoServlet</servlet-name>
    <servlet-class>ru.job4j.cartrade.controller.PhotoDownloadServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AdvServlet</servlet-name>
    <servlet-class>ru.job4j.cartrade.controller.AdvServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PhotoUploadServlet</servlet-name>
    <servlet-class>ru.job4j.cartrade.controller.PhotoUploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PhotoUploadServlet</servlet-name>
    <url-pattern>/photoUpload.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AdvServlet</servlet-name>
    <url-pattern>/adv.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DownloadPhotoServlet</servlet-name>
    <url-pattern>/download.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CarServlet</servlet-name>
    <url-pattern>/car.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/index.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AuthServlet</servlet-name>
    <url-pattern>/auth.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegServlet</servlet-name>
    <url-pattern>/reg.do</url-pattern>
  </servlet-mapping>
</web-app>
